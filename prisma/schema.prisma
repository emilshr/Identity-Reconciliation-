// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum LinkPreference {
  SECONDARY
  PRIMARY
}

model Contact {
  id             Int            @id @default(autoincrement())
  phoneNumber    String?
  email          String?
  linkedId       Int?
  linkPreference LinkPreference
  createdAt      DateTime
  updatedAt      DateTime
  deletedAt      DateTime?
}
